<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.TilePane?>
<?import java.net.*?>

<TilePane fx:controller="controllers.ProjectController" xmlns:fx="http://javafx.com/fxml/1"
          prefTileWidth="200" prefTileHeight="150" hgap="10" vgap="10">
    <children>
        <Label text="Projects" />
        <Button text="Add Project" onAction="#addProject" />
        <Button text="Back" onAction="#goBack" styleClass="back-button" GridPane.rowIndex="2" GridPane.columnIndex="0"/>
    </children>
    <stylesheets>
        <URL value="@./styles.css" />
    </stylesheets>
</TilePane>
