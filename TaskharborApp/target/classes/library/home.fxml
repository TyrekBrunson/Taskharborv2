<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" fx:id="Library_pane" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21.0.1" fx:controller="controllers.HomeController">
    <children>
        <ImageView fx:id="imageView" fitHeight="100.0" fitWidth="100.0" layoutX="31.0" layoutY="14.0" AnchorPane.bottomAnchor="686.0" AnchorPane.leftAnchor="31.0" AnchorPane.rightAnchor="32.0" AnchorPane.topAnchor="14.0">
            <image>
                <Image url="@../images/Anchor.png" />
            </image>
        </ImageView>
        <Button fx:id="button" onAction="#onLoginClicked" text="Login" AnchorPane.bottomAnchor="100.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
        <Button fx:id="button1" onAction="#onSignupClicked" text="Signup" AnchorPane.bottomAnchor="50.0" AnchorPane.leftAnchor="50.0" AnchorPane.rightAnchor="50.0" />
    </children>
    <stylesheets>
        <URL value="@styles.css" />
    </stylesheets>
</AnchorPane>
